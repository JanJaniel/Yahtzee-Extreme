<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="endPane" stylesheets="@../styles/GameOver.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.yahtzeeextreme.GameOverController">
    <children>

      <Label layoutX="100.0" layoutY="23.0" styleClass="gameover" text="Game Over " />
      <Label fx:id="winner" layoutX="160.0" layoutY="85.0" prefHeight="58.0" prefWidth="181.0" styleClass="winner" text="Player 1 wins!" />
      <Button layoutX="27.0" layoutY="347.0" mnemonicParsing="false" onAction="#switchToMainMenu" styleClass="play" text=" Main Menu" />

        <ImageView fitHeight="140.0" fitWidth="140.0" layoutX="180.0" layoutY="175.0">
            <image>
                <Image url="@../images/ahmeteroll.gif" />
            </image>
        </ImageView>

    </children>
</AnchorPane>
